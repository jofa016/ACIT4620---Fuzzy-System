    {
      "cell_type": "code",
      "source": [
        "import numpy as np, pandas as pd\n",
        "from dataclasses import dataclass\n",
        "from typing import Dict\n",
        "\n",
        "# -------- 0) Load data (ensure 'df' exists) --------\n",
        "# If you've already loaded `data`, reuse it; else read the CSV.\n",
        "if 'data' in globals():\n",
        "    df = data.copy()\n",
        "else:\n",
        "    file_path = \"./data/house1_2013_1min-kopi.csv\"\n",
        "    df = pd.read_csv(file_path, parse_dates=[\"timestamp\"], index_col=\"timestamp\")\n",
        "\n",
        "# Ensure aggregate is numeric and 1-min grid\n",
        "df['aggregate'] = pd.to_numeric(df['aggregate'], errors='coerce')\n",
        "df = df[df['aggregate'].notna()].sort_index().asfreq('1min')\n",
        "\n"
      ],
      "metadata": {
        "id": "Zirv8i3rAXDZ"
      },
